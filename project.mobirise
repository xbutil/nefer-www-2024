          "_customHTML": "<section class=\"menu1 medicalm4_menu1\" group=\"Menu\" plugins=\"DropDown, TouchSwipe, font-awesome\" global always-top position-absolute not-draggable once=\"menu\">\n\n\t<mbr-parameters>\n\t\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"15\" step=\"1\" value=\"0\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"15\" step=\"1\" value=\"0\">\n\n\t\t<input type=\"range\" title=\"Logo Size\" inline name=\"logoSize\" min=\"3.8\" max=\"8\" step=\"0.1\" value=\"4.3\" condition=\"showLogo\">\n\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Top Info\" name=\"showTopContent\">\n\t\t<input type=\"checkbox\" title=\"Left Side\" name=\"showLeftTopSide\" condition=\"showTopContent\">\n\t\t<input type=\"checkbox\" title=\"Right Side\" name=\"showRightTopSide\" condition=\"showTopContent\" checked>\n\t\t<input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked>\n\t\t<input type=\"checkbox\" title=\"Brand Name\" name=\"showBrand\">\n\t\t<input type=\"checkbox\" title=\"Menu Items\" name=\"showItems\" checked>\n\t\t<input type=\"checkbox\" title=\"Top Icons\" name=\"showIcons\" checked>\n\t\t<input type=\"checkbox\" title=\"Menu Icons\" name=\"showMenuIcons\" checked>\n\t\t<input type=\"range\" title=\"Icons Count\" inline name=\"countIcons\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showTopContent\">\n\t\t<input type=\"checkbox\" title=\"Button(s)\" name=\"showButtons\">\n\t\t<hr>\n\t\t<input type=\"checkbox\" title=\"Sticky\" name=\"sticky\" checked>\n\t\t<input type=\"checkbox\" title=\"Collapsed\" name=\"collapsed\">\n\t\t<input type=\"checkbox\" title=\"Transparent\" name=\"transparent\">\n\t\t<header>Colors</header>\n\t\t<input type=\"color\" title=\"Items Hover & Dropdown\" name=\"itemsHoverColor\" value=\"#000000\" condition=\"showItems\">\n\t\t<input type=\"color\"