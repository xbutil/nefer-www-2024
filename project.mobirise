ion value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"range\" inline title=\"№ 1\" name=\"progress1\" min=\"0\" max=\"100\" step=\"1\" value=\"100\">\n        <input type=\"range\" inline title=\"№ 2\" name=\"progress2\" min=\"0\" max=\"100\" step=\"1\" value=\"100\" condition=\"progressCount>1\">\n        <input type=\"range\" inline title=\"№ 3\" name=\"progress3\" min=\"0\" max=\"100\" step=\"1\" value=\"100\" condition=\"progressCount>2\">\n        <input type=\"range\" inline title=\"№ 4\" name=\"progress4\" min=\"0\" max=\"100\" step=\"1\" value=\"11\" condition=\"progressCount>3\">\n\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\">\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background3.png\" parallax selected>\n            <input type=\"color\" title=\"Background Color\" value=\"#4f87fb\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=l8UXhtqV6hU\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#4f87fb\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"main container\">\n        <div class=\"row\">\n            <div class=\"row content-row col-lg-5\" mbr-class=\"{'order-2':reverseContent}\">\n                <h3 class=\"mbr-section-subtitle mbr-fonts-style align-center mbr-bold\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">EXPERIENCIA CERTIICADA</h3>\n                <h2 class=\"mbr-section-title mbr-fonts-style mbr-bold align-left\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">Periodontal Therapy Skills.</h2>\n                <h4 class=\"mbr-text mbr-fonts-style mb-3\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text,.mbr-section-btn\">\n                    Lorem ipsum dolor sit amet, consectetur adipisicing elit. Iste blanditiis accusamus provident nulla\n                    aspernatur tenetur nisi eaque esse optio, animi pariatur rem ratione totam eius fugit doloremque\n                    quas, fuga magni.\n                </h4>\n                <div class=\"video-wrap d-flex align-items-center w-100\" mbr-if=\"showButtons\" mbr-class=\"{'justify-content-start': justify == 1, 'justify-content-center': justify == 2, 'justify-content-end': justify == 3}\">\n                    <div class=\"mbr-media show-modal align-center d-flex align-items-center\" data-modal=\".modalWindow\" mbr-if=\"showButtons\">\n                        <div class=\"icon-wrap\" mbr-if=\"showButtons\">\n                            <span mbr-icon class=\"mbr-iconfont fa-play fa\" mbr-if=\"showButtons\"></span>\n                        </div>\n                    </div>\n                    <h4 class=\"video-text mbr-fonts-style align-self-center ml-3 my-auto\" mbr-theme-style=\"display-4\" data-app-selector=\".video-wrap .video-text,.mbr-section-btn\" mbr-if=\"showButtons\">\n                        Ver nuestro video</h4>\n                </div>\n            </div>\n\n            <div class=\"second-col col-lg-7\">\n                <!--  pt-5 mt-2 -->\n                <div class=\"row justify-content-center\">\n                    <div class=\"card align-center\">\n                        <div class=\"wrap\">\n                            <div class=\"pie_progress progress1\" role=\"progressbar\" data-goal=\"{{progress1}}\">\n                                <p class=\"pie_progress__number mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\">100%</p>\n                            </div>\n                        </div>\n                        <div class=\"mbr-crt-title pt-3\">\n                            <h4 class=\"card-title py-2 mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card .mbr-crt-title .card-title\">Pacientes satisfechos</h4>\n                        </div>\n                    </div>\n\n                    <div class=\"card align-center\" mbr-if=\"progressCount>1\">\n                        <div class=\"wrap\">\n                            <div class=\"pie_progress progress2\" role=\"progressbar\" data-goal=\"{{progress2}}\">\n                                <p class=\"pie_progress__number mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\">100%</p>\n                            </div>\n                        </div>\n                        <div class=\"mbr-crt-title pt-3\">\n                            <h4 class=\"card-title py-2 mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card .mbr-crt-title .card-title\">\n                                Cirujanos certificados</h4>\n                        </div>\n                    </div>\n\n                    <div class=\"card align-center\" mbr-if=\"progressCount>2\">\n                        <div class=\"wrap\">\n                            <div class=\"pie_progress progress3\" role=\"progressbar\" data-goal=\"{{progress3}}\">\n                                <p class=\"pie_progress__number mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\">53%\n                                </p>\n                            </div>\n                        </div>\n                        <div class=\"mbr-crt-title pt-3\">\n                            <h4 class=\"card-title py-2 mbr-fonts-style  mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card .mbr-crt-title .card-title\">\n                                Servicios 24 horas</h4>\n                        </div>\n                    </div>\n\n                    <div class=\"card align-center\" mbr-if=\"progressCount>3\">\n                        <div class=\"wrap\">\n                            <div class=\"pie_progress progress4\" role=\"progressbar\" data-goal=\"{{progress4}}\">\n                                <p class=\"pie_progress__number mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\">\n                                </p>\n                            </div>\n                        </div>\n                        <div class=\"mbr-crt-title pt-3\">\n                            <h4 class=\"card-title py-2 mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".card .mbr-crt-title .card-title\">\n                                Safety</h4>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div>\n        <div class=\"modalWindow\" style=\"display: none;\">\n            <div class=\"modalWindow-container\">\n                <div class=\"modalWindow-video-container\">\n                    <div class=\"modalWindow-video\">\n                        <iframe width=\"100%\" height=\"100%\" data-src=\"{{modalVideo}}\" frameborder=\"0\" allowfullscreen=\"1\"></iframe>\n                    </div>\n                    <a class=\"close\" role=\"button\" data-dismiss=\"modal\">\n                        <span aria-hidden=\"true\" class=\"mbri-close mbr-iconfont closeModal\"></span>\n                        <span class=\"sr-only\">Close</span>\n                    </a>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
